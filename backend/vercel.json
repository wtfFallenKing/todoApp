{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ],
  "functions": {
    "src/main.ts": {
        "runtime": "@vercel/node",
        "maxDuration": 30
    }
  }
}